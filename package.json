{
    "name": "langium-simple-ui",
    "displayName": "langium-simple-ui",
    "description": "description",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "simple-ui",
            "aliases": ["SimpleUI", "simple-ui"],
            "extensions": [".sui"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "simple-ui",
            "scopeName": "source.simple-ui",
            "path": "./syntaxes/simple-ui.tmLanguage.json"
        }]
    },
    "activationEvents": [
        "onLanguage:simple-ui"
    ],
    "files": [
        "bin",
        "out",
        "src"
    ],
    "bin": {
        "simple-ui-cli": "./bin/cli"
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run build && npm run lint",
        "build": "webpack",
        "watch": "webpack --watch",
        "lint": "eslint src --ext ts",
        "langium:generate": "langium generate"
    },
    "dependencies": {
        "commander": "^8.0.0",
        "langium": "0.2.0-next.1f60202",
        "vscode-languageclient": "^7.0.0",
        "vscode-languageserver": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^14.17.3",
        "@types/vscode": "^1.56.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "langium-cli": "0.2.0-next.17bdb70",
        "source-map-loader": "^3.0.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.1.3",
        "webpack": "^5.39.1",
        "webpack-cli": "^4.7.2"
    }
}
